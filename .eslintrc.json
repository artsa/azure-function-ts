{
  "root": true,
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "globalReturn": true
    }
  },
  "rules": {
    "indent": "error",
    "semi": "error",
    "no-unused-vars": "error",
    "no-mixed-spaces-and-tabs": "error"
  }
}
